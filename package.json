{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "zod-prisma": "bin/cli.js"
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "install-peers": "install-self-peers -- --ignore-scripts",
    "prepare": "yarn install-peers && yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": true
  },
  "name": "zod-prisma",
  "author": "Carter Grimmeisen",
  "module": "dist/zod-prisma.esm.js",
  "size-limit": [
    {
      "path": "dist/zod-prisma.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/zod-prisma.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@team-griffin/install-self-peers": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1-alpha.0",
    "@typescript-eslint/parser": "^4.26.1-alpha.0",
    "husky": "^6.0.0",
    "jest-mock-extended": "^1.0.15",
    "prettier": "^2.3.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/generator-helper": "^2.24.0",
    "change-case": "^4.1.2",
    "ts-morph": "^11.0.0"
  }
}
