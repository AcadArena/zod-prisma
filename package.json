{
	"name": "@acadarena/zod-prisma",
	"version": "0.8.0",
	"description": "A Prisma generator that creates Zod schemas for all of your models",
	"license": "MIT",
	"author": "Carter Grimmeisen",
	"homepage": "https://github.com/AcadArena/zod-prisma#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/AcadArena/zod-prisma.git"
	},
	"bugs": {
		"url": "https://github.com/AcadArena/zod-prisma/issues"
	},
	"main": "dist/index.js",
	"module": "dist/zod-prisma.esm.js",
	"typings": "dist/index.d.ts",
	"bin": {
		"zod-prisma": "bin/cli.js"
	},
	"keywords": [
		"zod",
		"prisma",
		"generator"
	],
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "dts build --target node --format cjs --rollupTypes",
		"lint": "tsc --noEmit && dts lint src --ignore-pattern src/test/functional",
		"prepare": "husky install",
		"prepublish": "dts build --target node --format cjs --rollupTypes",
		"start": "dts watch",
		"test": "dts test --maxWorkers=4 --verbose",
		"release": "npx np"
	},
	"prettier": {
		"printWidth": 100,
		"semi": false,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"eslintConfig": {
		"rules": {
			"react-hooks/rules-of-hooks": "off"
		}
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"@prisma/generator-helper": "4.6.1",
		"parenthesis": "3.1.8",
		"ts-morph": "17.0.1"
	},
	"devDependencies": {
		"@prisma/client": "4.6.1",
		"@prisma/sdk": "4.0.0",
		"@tsconfig/recommended": "1.0.1",
		"@types/fs-extra": "9.0.13",
		"dts-cli": "1.6.0",
		"execa": "5.1.0",
		"fast-glob": "3.2.12",
		"fs-extra": "10.1.0",
		"husky": "8.0.2",
		"jest-mock-extended": "3.0.1",
		"prisma": "4.6.1",
		"tslib": "2.4.1",
		"typescript": "4.9.3",
		"zod": "3.19.1"
	},
	"peerDependencies": {
		"decimal.js": "^10.0.0",
		"prisma": "^3.0.0 || ^4.0.0",
		"zod": "^3.0.0"
	},
	"peerDependenciesMeta": {
		"decimal.js": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=14"
	},
	"publishConfig": {
		"access": "public"
	}
}
